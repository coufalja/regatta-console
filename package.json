{
	"name": "regatta-console",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "bunx prettier --plugin-search-dir . --check . && eslint .",
		"format": "bunx prettier --plugin-search-dir . --write .",
		"prototypes": "proto-loader-gen-types --bytes=String --longs=String --enums=String --defaults=true --oneofs=true --grpcLib=@grpc/grpc-js --outDir=./src/lib/proto ./src/lib/proto/*.proto"
	},
	"devDependencies": {
		"@catdadcode/svelte-adapter-bun": "^0.5.7",
		"@floating-ui/dom": "^1.5.3",
		"@iconify/json": "^2.2.161",
		"@skeletonlabs/skeleton": "^2.6.0",
		"@skeletonlabs/tw-plugin": "^0.3.0",
		"@sveltejs/kit": "^2.0.6",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@tailwindcss/forms": "^0.5.7",
		"@tailwindcss/typography": "^0.5.10",
		"@types/google-protobuf": "^3.15.12",
		"@typescript-eslint/eslint-plugin": "^6.14.0",
		"@typescript-eslint/parser": "^6.14.0",
		"autoprefixer": "^10.4.16",
		"eslint": "^8.55.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"grpc-tools": "^1.12.4",
		"grpc_tools_node_protoc_ts": "^5.3.3",
		"highlight.js": "^11.9.0",
		"postcss": "^8.4.32",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte": "^4.2.1",
		"svelte-check": "^3.5.2",
		"tailwindcss": "^3.3.6",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3",
		"unplugin-icons": "^0.18.1",
		"vite": "^5.0.10",
		"vite-plugin-tailwind-purgecss": "^0.2.0",
		"vitest": "^1.0.4"
	},
	"type": "module",
	"dependencies": {
		"@auth/sveltekit": "^0.5.0",
		"@grpc/grpc-js": "^1.9.13",
		"@grpc/proto-loader": "^0.7.10"
	}
}
